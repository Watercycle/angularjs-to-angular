{
    "env": {
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "rules": {

        "no-alert": 2,
        "no-caller": 2,
        "no-bitwise": 0,
        "no-console": 0,
        "no-debugger": 2,
        "no-empty": 2,
        "no-eval": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 0,
        "no-irregular-whitespace":0,
        "no-floating-decimal": 0,
        "no-implied-eval": 2,
        "no-with": 2,
        "no-fallthrough": 2,
        "no-unreachable": 2,
        "no-undef-init": 2,
        "no-octal": 2,
        "no-obj-calls": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-native-reassign": 2,
        "no-plusplus": 0,
        "no-delete-var": 2,
        "no-return-assign": 2,
        "no-new-object": 2,
        "no-label-var": 2,
        "no-ternary": 0,
        "no-self-compare": 0,
        "no-undef":0,
        "smarter-eqeqeq": 0,
        "brace-style": 0,
        "camelcase": 0,
        "curly": 2,
        "dot-notation": 0,
        "eqeqeq": 1,
        "new-parens": 2,
        "guard-for-in": 0,
        "radix": 0,
        "new-cap": 2,
        "quote-props": 0,
        "semi": 2,
        "use-isnan": 2,
        "quotes": [2, "single"],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "complexity": [0, 11],
        "wrap-iife": 0,
        "no-multi-str": 2
    }
}